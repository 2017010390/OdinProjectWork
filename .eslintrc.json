{
    "env": {
        "browser": true,
        "es2021": true,
        "commonjs": true,
        "node": true
    },
    "extends": [
        "airbnb-base",
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "semi": ["error", "always"],
        "quotes": ["error", "double"],
        "func-names": ["off"],
        "import/no-extraneous-dependencies": "off",
        "import/extensions": "off"
    }
}
